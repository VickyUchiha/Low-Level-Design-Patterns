{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (Clean Output)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net8.0/YourProject.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "logging": {
        "moduleLoad": false,
        "engineLogging": false,
        "exceptionLogging": false,
        "programOutput": true
      }
    }
  ]
}
